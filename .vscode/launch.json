{
    "configurations": [
        {
            "name": "Microvisor Remote",
            "cwd": "${workspaceRoot}",
            "type": "gdb",
            "remote": true,
            "target": "localhost:8001",
            "request": "attach",
            "preLaunchTask": "start_gdb_server",
            "gdbpath": "/usr/bin/gdb-multiarch",
            "debugger_args": ["-l", "10", "--nx"],
            "executable": "${workspaceRoot}/build/Demo/gpio_toggle_demo.elf",
            "printCalls": false,
            "valuesFormatting": "parseText",
            "showDevDebugOutput": false,
            "stopAtConnect": false
        }
    ],
    "version": "0.2.0"
}
